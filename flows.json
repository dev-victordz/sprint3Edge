    {
        "id": "01c1af62e0e2fbad",
        "type": "function",
        "z": "876e86ec8e42d839",
        "name": "verificaPontos",
        "func": "let pontosArray = msg.payload;\n\nif (!Array.isArray(pontosArray) || pontosArray.length === 0) {\n    msg.payload = \"red\";\n} else {\n    let usuarioPontos = pontosArray[0].pontos;\n\n    if (usuarioPontos > 500) {\n        msg.payload = \"green\";\n    } else {\n        msg.payload = \"red\";\n        msg.topic = null\n    }\n}\n\nreturn msg;",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
