        "type": "function",
        "z": "876e86ec8e42d839",
        "name": "capturaID",
        "func": "if (msg.payload.startsWith(\"MAIN_UID:\")) {\n    msg.payload = msg.payload.replace(\"MAIN_UID:\", \"\").trim();\n    var uid = msg.payload;\n    msg.topic = \"SELECT pontos FROM usuarios WHERE id = '\" + uid + \"';\";\n    return msg;\n}\nreturn null;",
